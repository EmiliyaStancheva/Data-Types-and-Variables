using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace Variable_in_Hex_Format
{
    class VariableInHexFormat
    {
        static void Main(string[] args)
        {
            string hexNumber1 = Console.ReadLine();
            int hexConverted1 = Convert.ToInt32(hexNumber1, 16);
            Console.WriteLine(hexConverted1);
            Console.Read();
        }
    }
}
